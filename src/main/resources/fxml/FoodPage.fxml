         </left>
      </BorderPane>
      <AnchorPane fx:id="homePane" layoutY="45.0" prefHeight="665.0" prefWidth="901.0">
         <children>
            <fx:include source="MainFood.fxml" />
         </children>
      </AnchorPane>
      <AnchorPane fx:id="profilePane" layoutY="45.0" prefHeight="665.0" prefWidth="901.0">
         <children>
            <fx:include source="ProfileFood.fxml" />
         </children>
      </AnchorPane>
      <AnchorPane fx:id="panierPane" layoutY="45.0" prefHeight="665.0" prefWidth="901.0">
         <children>
            <fx:include source="PanierFood.fxml" />
         </children>
      </AnchorPane>
