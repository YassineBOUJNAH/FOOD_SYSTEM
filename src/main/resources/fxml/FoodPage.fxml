                        <FontAwesomeIconView glyphName="USER" size="2em" />
                     </graphic>
                  </JFXButton>
                  <JFXButton fx:id="bar2" alignment="CENTER" contentDisplay="CENTER" onMouseClicked="#run2" prefHeight="39.0" prefWidth="48.0">
