                        <FontAwesomeIconView glyphName="SHOPPING_CART" size="2em" />
                     </graphic>
                  </JFXButton>
                  <JFXButton fx:id="ButtonProfile" layoutX="537.0" onAction="#OnProfileAction" prefHeight="38.0" prefWidth="141.0" styleClass="r" text="Profile">
